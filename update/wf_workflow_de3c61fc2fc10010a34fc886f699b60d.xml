    <sys_variable_value action="INSERT_OR_UPDATE">
        <document>wf_activity</document>
        <document_key>a642cdaddb610010ec759235ca961930</document_key>
        <order>1</order>
        <sys_class_name>sys_variable_value</sys_class_name>
        <sys_created_by>admin</sys_created_by>
        <sys_created_on>2019-12-05 21:13:09</sys_created_on>
        <sys_id>2e42cdaddb610010ec759235ca961930</sys_id>
        <sys_mod_count>1</sys_mod_count>
        <sys_updated_by>admin</sys_updated_by>
        <sys_updated_on>2019-12-05 21:13:16</sys_updated_on>
        <value>/*global tf_terraform_record, tf_util, tf_variable, tf_run, tf_http*/&#13;
&#13;
/*&#13;
  This workflow looks for any ServiceNow Terraform records that have set&#13;
  is_destroyable to true. This assumes that these Terraform Workspaces have&#13;
  been marked for deletion.&#13;
&#13;
  This workflow:&#13;
  - sets the is_destroyable value to true on the ServiceNow Terraform Table for the resource&#13;
  - creates the CONFIRM_DESTROY=1 environment variable in terraform for the workspace&#13;
  - creates a plan for a destroy run in terraform&#13;
&#13;
  The "Poll Destroy Workspace" finishes the process of cleaning up the workspace by applying&#13;
  the destroy run, then deleting the workspace from Terraform.&#13;
*/&#13;
&#13;
gs.info("Workflow: terraform delete workspace started...");&#13;
&#13;
var terraform_record = new tf_terraform_record();&#13;
var util             = new tf_util();&#13;
var tf_var           = new tf_variable();&#13;
var run              = new tf_run();&#13;
&#13;
/* Data for the request content */&#13;
var workspaceName = current.variables.workspace_name;  // Variable from Catalog Request&#13;
var tfWorkspace   = terraform_record.get(workspaceName);&#13;
&#13;
// Search Terraform Table for the record&#13;
var grTerraform = new GlideRecord("x_325709_terraform_terraform");&#13;
grTerraform.addQuery("workspace_name", workspaceName);&#13;
grTerraform.setLimit(1);&#13;
grTerraform.query();&#13;
while (grTerraform.next()) {&#13;
  var workspaceID = grTerraform.workspace_id;&#13;
&#13;
  // Mark the ServiceNow Terraform Resource Record as destroyable&#13;
  grTerraform.is_destroyable = true;&#13;
  grTerraform.update();&#13;
&#13;
	/* Create environment variable CONFIRM_DESTROY=1 */&#13;
  var var_content = tf_var.createVariableRequestContent(workspaceID, "CONFIRM_DESTROY", "1", "env", false);&#13;
  tf_var.createVariable(JSON.stringify(var_content));&#13;
&#13;
  /* Plan a Destroy Run */&#13;
  var destroyContent = run.createRunRequestContent(workspaceID, "Planning Destroy Run triggered by scheduler", true);&#13;
  var destroyReqJSON = JSON.stringify(destroyContent);&#13;
&#13;
  // HTTP REST request to Terraform Run API to create a plan for the destroy run&#13;
  var httpRun = new tf_http("TF Runs", "Create Run", destroyReqJSON);&#13;
  httpRun.setParam("workspaceID", workspaceID);&#13;
  httpRun.execute();&#13;
}&#13;
&#13;
/* Add comment to original workspace request item */&#13;
var url = "sc_req_item.do?sys_id=" + current.sys_id;&#13;
var comment = util.formatCommentBlockQuote("Terraform: Marked Workspace for delete...") + "Triggered by " + util.formatCommentHyperlink(url, current.getDisplayValue());&#13;
terraform_record.postRITMComment(tfWorkspace.requested_item, comment);&#13;
&#13;
/* Add comment to current request item */&#13;
url = "sc_req_item.do?sys_id=" + tfWorkspace.requested_item;&#13;
comment = "Marked Workspace for delete " + util.formatCommentHyperlink(url, workspaceName);&#13;
terraform_record.postRITMComment(current.sys_id, comment);&#13;
&#13;
gs.info("Workflow: terraform delete workspace finished.");&#13;
</value>
