    <sys_variable_value action="INSERT_OR_UPDATE">
        <document>wf_activity</document>
        <document_key>e47bb4cadbd23300176661ca0b9619d3</document_key>
        <order>1</order>
        <sys_class_name>sys_variable_value</sys_class_name>
        <sys_created_by>admin</sys_created_by>
        <sys_created_on>2019-06-28 20:36:09</sys_created_on>
        <sys_id>e87bb4cadbd23300176661ca0b9619d3</sys_id>
        <sys_mod_count>1</sys_mod_count>
        <sys_updated_by>admin</sys_updated_by>
        <sys_updated_on>2019-06-28 20:36:17</sys_updated_on>
        <value>/*global tf_run, tf_http, tf_terraform_record, tf_util*/&#13;
&#13;
gs.info("Workflow: Terraform Apply Run Started...");&#13;
&#13;
var run              = new tf_run();&#13;
var workspace        = new tf_workspace();&#13;
var terraform_record = new tf_terraform_record();&#13;
var requestID        = current.request.getDisplayValue(); // Catalog Request Number/ID&#13;
var workspace_name   = current.variables.workspace_name;  // Variable from Catalog Request&#13;
var util             = new tf_util();&#13;
&#13;
var runID;&#13;
var currentRun = workspace.getWorkspaceCurrentRun(workspace_name);&#13;
if (currentRun !== null) {&#13;
    runID = currentRun.id;&#13;
} else {&#13;
  gs.error("Run State is null for: " + requestID);&#13;
}&#13;
&#13;
/* Data for the request content */&#13;
var tf_workspace   = terraform_record.get(workspace_name);&#13;
var workspace_id   = String(tf_workspace.workspace_id);&#13;
var request_id     = "RequestID: " + requestID;&#13;
&#13;
var content        = run.createRunRequestContent(workspace_id, request_id);&#13;
var requestContent = JSON.stringify(content);&#13;
&#13;
/* HTTP REST request to Terraform API */&#13;
var http = new tf_http("TF Runs", "Apply Run", requestContent);&#13;
http.setParam("run_id", String(runID));&#13;
http.execute();&#13;
&#13;
/* Add comment to original workspace request item */&#13;
var url = "sc_req_item.do?sys_id=" + current.sys_id;&#13;
var comment = util.formatCommentBlockQuote("Terraform: Applying Run...") + "Triggered by " + util.formatCommentHyperlink(url, current.getDisplayValue());&#13;
terraform_record.postRITMComment(tf_workspace.requested_item, comment);&#13;
&#13;
/* Add comment to current request item */&#13;
url = "sc_req_item.do?sys_id=" + tf_workspace.requested_item;&#13;
comment = "Apply run triggerd for workspace " + util.formatCommentHyperlink(url, workspace_name);&#13;
terraform_record.postRITMComment(current.sys_id, comment);&#13;
&#13;
gs.info("Workflow: Terraform Apply Run Finished.");&#13;
</value>
