        <sys_updated_on>2019-12-05 21:03:01</sys_updated_on>
        <value>/*global tf_workspace, tf_run, tf_terraform_record, tf_http*/&#13;
/*&#13;
  Workspace Polling&#13;
    Finds any records not in an applied state then&#13;
    queries the Terraform Enterprise API for their status.&#13;
*/&#13;
&#13;
// System Property variables for Terraform API&#13;
var terraformRecord = new tf_terraform_record();&#13;
var run             = new tf_run();&#13;
var workspace       = new tf_workspace();&#13;
&#13;
// Search Terraform Table for any records not in "applied" state&#13;
var gr = new GlideRecord("x_325709_terraform_terraform");&#13;
gr.addQuery("last_known_state", "=", "planned ");&#13;
gr.query();&#13;
&#13;
// Loop through all records&#13;
while (gr.next()) {&#13;
  var currentRun = workspace.getWorkspaceCurrentRun(gr.workspace_name);&#13;
  var runID      = currentRun.id;&#13;
&#13;
  /* Data for the request content */&#13;
  var tfWorkspace = terraformRecord.get(gr.workspace_name);&#13;
  var workspaceID = String(tfWorkspace.workspace_id);&#13;
  var runMesssage = "Triggered by scheduler";&#13;
&#13;
  var content        = run.createRunRequestContent(workspaceID, runMesssage);&#13;
  var requestContent = JSON.stringify(content);&#13;
&#13;
  /* HTTP REST request to Terraform API */&#13;
  var http = new tf_http("TF Runs", "Apply Run", requestContent);&#13;
  http.setParam("run_id", String(runID));&#13;
  http.execute();&#13;
} // end while loop&#13;
</value>
